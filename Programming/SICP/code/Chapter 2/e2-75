(define (make-from-mag-ang r a) 
	(define (magnitude z) (car z))
	(define (angle z) (cdr z))
	(define (dispatch op)
		(cond ((eq? op 'real-part) (* (magnitude z) (cos (angle z)))) 
		((eq? op 'imag-part) (* (magnitude z) (sin (angle z))))
		((eq? op 'magnitude) r) 
		((eq? op 'angle) a)
		(else (error "Unknown op: MAKE-FROM-MAG-AND" op))))
	dispatch)
