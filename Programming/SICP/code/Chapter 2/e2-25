(define (first items)
	(car (cdr (car (cdr (cdr items)))))
)
(define (second items)
	(car (car items))
)
(define (third items)
	(car (cdr (car (cdr (car (cdr (car (cdr (car (cdr (car (cdr items))))))))))))
)

(first (list 1 3 (list 5 7) 9))
(second (list (list 7)))
(third (list 1 (list 2 (list 3 (list 4 (list 5 (list 6 7)))))))