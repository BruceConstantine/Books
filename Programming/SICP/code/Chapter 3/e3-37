(define (c+ x y)
(let ((z (make-connector)))
          (adder x y z)
          z))

(define (c- x y)
(let ((z (make-connector)))
          (subtract x y z)
          z))

(define (c* x y)
(let ((z (make-connector)))
          (mult x y z)
          z))

(define (c/ x y)
(let ((z (make-connector)))
          (divi x y z)
          z))

(define (cv x)
(let ((z (make-connector)))
          (constant x z)
          z))