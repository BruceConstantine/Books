ev-cond
	(assign unev (reg exp))
	(assign exp (op first-cond) (reg unev))
	(test (op last-cond?) (reg exp))
	(branch (label-last-cond))
	(assign unev (op rest-cond) (reg unev))
	(test (op last-cond?) (exp))
	(save exp)
	(assign proc (op get-body) (reg exp))
	(assign exp (op get-test) (reg exp))
	(save env)
	(save unev)
	(save proc)
	(save continue)
	(assign continue (label cond-1))
	(goto (eval-dispatch))
cond-1
	(restore continue)
	(restore proc)
	(restore unev)
	(restore env)
	(restore exp)
	(test (op true?) val)
	(branch ev-cond)
	(assign env proc)
	(goto (label ev-sequence))
last-cond
	(assign exp (op get-body) (reg exp))
	(goto (label ev-sequence))