(define (install-and-or)
	(define (eval-and exp env)
		(if ((get 'eval (caar exp)) (car exp) env)
			(if (null? (cdr exp))
				((get 'eval (caar exp)) (car exp) env)
				(eval-and (cdr exp) env)
			)
			false
		)
	)
	(define (eval-or exp env)
		(if ((get 'eval (caar exp)) (car exp) env)
			((get 'eval (caar exp)) (car exp) env)
			(if (null? (cdr exp))
				false
				(eval-or (cdr exp) env)
			)
		)
	)
	(put 'eval 'and eval-and)
	(put 'eval 'or eval-or)
)